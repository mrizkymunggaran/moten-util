			<xs:element name="periodMonths" type="xs:integer" />
			<xs:element name="periodDays" type="xs:integer" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="courseDetails">
		<xs:sequence>
			<xs:element name="personalSafetyAndSocialResponsibility"
				type="courseDetail" />
